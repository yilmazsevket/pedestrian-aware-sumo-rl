  <flow id="early_night_E2S" type="passenger_car" route="E2S" begin="0" end="25200" probability="0.001" departPos="random" departSpeed="max"/>
  <flow id="early_night_W2E" type="passenger_car" route="W2E" begin="0" end="25200" probability="0.005" departPos="random" departSpeed="max"/>
  <flow id="early_night_W2N" type="passenger_car" route="W2N" begin="0" end="25200" probability="0.001" departPos="random" departSpeed="max"/>
