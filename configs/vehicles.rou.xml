  <!-- SUMO vehicle routes: vehicle types, routes, and time-of-day flows for a 4-arm junction with all turning options. -->
  <!-- Vehicle types with more variation -->
  <vType id="passenger_car" vClass="passenger" length="5.0" minGap="2.0" maxSpeed="40.0" accel="2.0" decel="4.0" sigma="0.8" speedDev="0.15" color="1,1,0"/>
  <vType id="heavy_vehicle" vClass="truck" length="10.0" minGap="3.0" maxSpeed="25.0" accel="1.0" decel="3.0" sigma="0.6" speedDev="0.1" color="0,0,1"/>

  <!-- Route definitions (with all turning options) -->
  <!-- From North (-E0) -->
  <route id="N2S" edges="-E0 E2" color="1,0,0"/>      <!-- Straight -->
  <route id="N2E" edges="-E0 E3" color="1,0.5,0"/>    <!-- Right turn -->
  <route id="N2W" edges="-E0 E1" color="1,0,0.5"/>    <!-- Left turn -->
  
  <!-- From South (-E2) -->
  <route id="S2N" edges="-E2 E0" color="0,1,0"/>      <!-- Straight -->
  <route id="S2W" edges="-E2 E1" color="0.5,1,0"/>    <!-- Right turn -->
  <route id="S2E" edges="-E2 E3" color="0,1,0.5"/>    <!-- Left turn -->
  
  <!-- From East (-E3) -->
  <route id="E2W" edges="-E3 E1" color="0,0,1"/>      <!-- Straight -->
  <route id="E2N" edges="-E3 E0" color="0.5,0,1"/>    <!-- Right turn -->
  <route id="E2S" edges="-E3 E2" color="0,0.5,1"/>    <!-- Left turn -->
  
  <!-- From West (-E1) -->
  <route id="W2E" edges="-E1 E3" color="1,1,1"/>      <!-- Straight -->
  <route id="W2S" edges="-E1 E2" color="1,0.5,1"/>    <!-- Right turn -->
  <route id="W2N" edges="-E1 E0" color="0.5,1,1"/>    <!-- Left turn -->

  <!-- NIGHT TRAFFIC PART 1 (00:00-07:00) with low volume -->
