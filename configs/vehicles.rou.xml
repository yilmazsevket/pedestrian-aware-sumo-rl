  <flow id="early_night_S2E" type="passenger_car" route="S2E" begin="0" end="25200" probability="0.0015" departPos="random" departSpeed="max"/>
  <flow id="early_night_S2W" type="passenger_car" route="S2W" begin="0" end="25200" probability="0.0015" departPos="random" departSpeed="max"/>
